{
    "name": "Cyber Planet",
    "version": "1.0.0",
    "description": "",
    "manifest_version": 3,
    "incognito": "split",
    "icons": {
        "128": "icons/128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/check-lowest.js"
            ],
            "matches": [
                "*://*.magiceden.io/marketplace/*"
            ],
           
            "run_at": "document_end",
            "css": [
                "theme/styles.css"
            ]
        }
    ],
    "action": {
        "default_icon": "icons/128.png",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
      "*://*.magiceden.io/*"
     ]
    
}